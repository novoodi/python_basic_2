       "f    pass\n",
       "g    pass\n",
       "h    fail\n",
       "i    fail\n",
       "j    pass\n",
       "dtype: object"
      ]
     },
     "execution_count": 112,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "# 익명함수로 코딩\n",
    "# 굳이 함수를 생성하지 않고 lambda를 사용 (익명 함수)\n",
    "sample.apply(lambda val: 'pass' if val >= 60 else 'fail') "
   ]
